{
    "version": 2,
    "public": false,
    "builds": [
      {
        "src": "./index.py",
        "use": "@vercel/python"
      },
      {
        "src": "/static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.py"
      }
    ]
  }